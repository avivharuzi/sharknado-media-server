{
  "name": "sharknado-media-server",
  "version": "1.0.0",
  "description": "Sharknado a personal media server",
  "main": "src/main.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "start": "npm run build && nodemon dist/main.js",
    "start:dev": "cross-env NODE_ENV=development concurrently \"npm:start\" \"npm:watch\"",
    "start:prod": "cross-env NODE_ENV=production npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avivharuzi/sharknado-media-server.git"
  },
  "keywords": [
    "sharknado",
    "media",
    "server"
  ],
  "author": "Aviv Haruzi <avivharuzi@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/avivharuzi/sharknado-media-server/issues"
  },
  "homepage": "https://github.com/avivharuzi/sharknado-media-server#readme",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.3",
    "@types/node": "^13.9.2",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.2",
    "ts-node": "^8.7.0",
    "typescript": "^3.8.3"
  }
}
